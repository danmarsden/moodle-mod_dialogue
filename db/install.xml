<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/dialogue/db" VERSION="2010123106" COMMENT="XMLDB file for Moodle mod/dialogue"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="dialogue" COMMENT="Dialogue" NEXT="dialogue_conversations">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="deleteafter"/>
        <FIELD NAME="deleteafter" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="course" NEXT="dialoguetype"/>
        <FIELD NAME="dialoguetype" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="deleteafter" NEXT="multipleconversations"/>
        <FIELD NAME="multipleconversations" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="dialoguetype" NEXT="maildefault"/>
        <FIELD NAME="maildefault" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="multipleconversations" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="maildefault" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="timemodified" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="Default comment for the field, please edit me" PREVIOUS="name" NEXT="introformat"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="text format of intro field" PREVIOUS="intro" NEXT="edittime"/>
        <FIELD NAME="edittime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="introformat"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for label"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="dialogue_conversations" COMMENT="Dialogue Conversations" PREVIOUS="dialogue" NEXT="dialogue_entries">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" ENUM="false" COMMENT="id of the table, please edit me" NEXT="dialogueid"/>
        <FIELD NAME="dialogueid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="Default comment for the field, please edit me" PREVIOUS="dialogueid" NEXT="recipientid"/>
        <FIELD NAME="recipientid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="lastid"/>
        <FIELD NAME="lastid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="recipientid" NEXT="lastrecipientid"/>
        <FIELD NAME="lastrecipientid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="lastid" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="lastrecipientid" NEXT="closed"/>
        <FIELD NAME="closed" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timemodified" NEXT="seenon"/>
        <FIELD NAME="seenon" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="closed" NEXT="ctype"/>
        <FIELD NAME="ctype" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="seenon" NEXT="format"/>
        <FIELD NAME="format" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="ctype" NEXT="subject"/>
        <FIELD NAME="subject" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="format" NEXT="groupid"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="subject" NEXT="grouping"/>
        <FIELD NAME="grouping" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="groupid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table, please edit me"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="dialogue_conversations_timemodified_idx" UNIQUE="false" FIELDS="timemodified" NEXT="dialogue_conversations_dialogueid_idx"/>
        <INDEX NAME="dialogue_conversations_dialogueid_idx" UNIQUE="false" FIELDS="dialogueid" PREVIOUS="dialogue_conversations_timemodified_idx" NEXT="dialogue_conversations_userid_idx"/>
        <INDEX NAME="dialogue_conversations_userid_idx" UNIQUE="false" FIELDS="userid" PREVIOUS="dialogue_conversations_dialogueid_idx" NEXT="dialogue_conversations_recipientid_idx"/>
        <INDEX NAME="dialogue_conversations_recipientid_idx" UNIQUE="false" FIELDS="recipientid" PREVIOUS="dialogue_conversations_userid_idx" NEXT="dialogue_conversations_groupid_idx"/>
        <INDEX NAME="dialogue_conversations_groupid_idx" UNIQUE="false" FIELDS="groupid" PREVIOUS="dialogue_conversations_recipientid_idx" NEXT="dialogue_conversations_grouping_idx"/>
        <INDEX NAME="dialogue_conversations_grouping_idx" UNIQUE="false" FIELDS="grouping" PREVIOUS="dialogue_conversations_groupid_idx"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="dialogue_entries" COMMENT="Dialogue entries" PREVIOUS="dialogue_conversations" NEXT="dialogue_read">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="true" ENUM="false" COMMENT="id of the table, please edit me" NEXT="dialogueid"/>
        <FIELD NAME="dialogueid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="conversationid"/>
        <FIELD NAME="conversationid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="dialogueid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="conversationid" NEXT="recipientid"/>
        <FIELD NAME="recipientid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="recipientid" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timecreated" NEXT="mailed"/>
        <FIELD NAME="mailed" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="timemodified" NEXT="text"/>
        <FIELD NAME="text" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="mailed" NEXT="format"/>
        <FIELD NAME="format" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="text" NEXT="trust"/>
        <FIELD NAME="trust" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="format" NEXT="attachment"/>
        <FIELD NAME="attachment" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="trust"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table, please edit me"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="dialogue_entries_conversationid_idx" UNIQUE="false" FIELDS="conversationid" NEXT="dialogue_entries_dialogueid_idx"/>
        <INDEX NAME="dialogue_entries_dialogueid_idx" UNIQUE="false" FIELDS="dialogueid" PREVIOUS="dialogue_entries_conversationid_idx" NEXT="dialogue_entries_userid_idx"/>
        <INDEX NAME="dialogue_entries_userid_idx" UNIQUE="false" FIELDS="userid" PREVIOUS="dialogue_entries_dialogueid_idx" NEXT="dialogue_entries_recipientid_idx"/>
        <INDEX NAME="dialogue_entries_recipientid_idx" UNIQUE="false" FIELDS="recipientid" PREVIOUS="dialogue_entries_userid_idx"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="dialogue_read" COMMENT="Records when dialogue entries were viewed" PREVIOUS="dialogue_entries">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="entryid"/>
        <FIELD NAME="entryid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" ENUM="false" PREVIOUS="entryid" NEXT="firstread"/>
        <FIELD NAME="firstread" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="lastread"/>
        <FIELD NAME="lastread" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" ENUM="false" PREVIOUS="firstread" NEXT="conversationid"/>
        <FIELD NAME="conversationid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" ENUM="false" PREVIOUS="lastread"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="dialogueread_entryid_userid_uk"/>
        <KEY NAME="dialogueread_entryid_userid_uk" TYPE="unique" FIELDS="entryid, userid" PREVIOUS="primary" NEXT="dialogueread_conversation_fk"/>
        <KEY NAME="dialogueread_conversation_fk" TYPE="foreign" FIELDS="conversationid" REFTABLE="dialogue_conversations" REFFIELDS="id" PREVIOUS="dialogueread_entryid_userid_uk"/>
      </KEYS>
    </TABLE>
  </TABLES>
  
</XMLDB>